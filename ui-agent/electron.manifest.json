{
  "aspCoreBackendPort": 19871,
  "executable": "treecat",
  "splashscreen": {
    "imageFile": ""
  },
  "name":  "treecat",
  "singleInstance": true,
  "build": {
    "appId": "com.treecat.app",
    "productName": "treecat",
    "copyright": "Copyright TTINAGI LLC Â© 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}